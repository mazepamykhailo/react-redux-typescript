{
  "name": "react-project-template",
  "version": "1.0.0",
  "description": "template for react projects",
  "main": "index.ts",
  "author": "Matt Nedrich",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --content-base ./",
    "test": "jest",
    "lint": "tslint -c tslint.json './client/**/*.ts*'"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "dependencies": {
    "@types/react": "^15.0.14",
    "@types/react-dom": "^0.14.23",
    "@types/react-redux": "^4.4.37",
    "@types/redux": "^3.6.0",
    "lodash": "^4.17.4",
    "path": "^0.12.7",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "redux": "^3.6.0",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1"
  },
  "devDependencies": {
    "@types/enzyme": "^2.8.0",
    "@types/jest": "^18.1.1",
    "@types/react-addons-test-utils": "^0.14.17",
    "@types/react-test-renderer": "^15.5.0",
    "awesome-typescript-loader": "^3.0.8",
    "enzyme": "^2.8.2",
    "jest": "^19.0.2",
    "react-addons-test-utils": "^15.4.2",
    "react-test-renderer": "^15.5.4",
    "source-map-loader": "^0.1.6",
    "ts-jest": "^19.0.0",
    "tslint": "^4.5.1",
    "tslint-eslint-rules": "^4.0.0",
    "typescript": "^2.3.4"
  }
}
